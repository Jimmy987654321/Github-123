#include <stdio.h>
#include <stdlib.h>
int main()
{
    char name[] = "張任翔";
    int age = 18;
    char blood = 'O';
    float height = 168.5;
    double money = 1234567000;
    printf("123456789012345678901234567890");
    printf("1234567890\n");
    printf("My name is %s ,\t I am %d years old.\n",name,age);
    printf("And my blood type is %c,\t %f height\n",blood,height);
    printf("I have %E dollors.\n",money);

    return 0;
}